# Sample environment variables for txstress
# You can rename this file to .env and modify it with your values

# Default node URL (can be overridden with --node flag)
NODE_URL=https://sepolia.infura.io/v3/YOUR_API_KEY

# Default keys file path (can be overridden with --keys flag)
KEYS_FILE=./keys.txt

# Default recipient address (can be overridden with --to flag)
DEFAULT_RECIPIENT=0x0000000000000000000000000000000000000000
